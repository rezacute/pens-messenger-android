<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#EAE9E6" />
            <stroke
                android:width="1dp"
                android:color="#FDFDFD" />
            <corners
                android:radius="1dp" />

        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#EAE9E6"
                android:endColor="#FDFDFD"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#FDFDFD" />
            <corners
                android:radius="1dp" />

        </shape>
    </item>
</selector>